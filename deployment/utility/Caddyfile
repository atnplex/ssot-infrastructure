# Routes public domains to internal containers

vault.atnplex.cloud {
	reverse_proxy vaultwarden:80
}

vault.atnplex.cloud:3012 {
	reverse_proxy vaultwarden:3012
}

bws.atnplex.cloud {
	reverse_proxy bws-server:5000
}

llm.atnplex.cloud {
	reverse_proxy litellm:4000
}

status.atnplex.cloud {
	reverse_proxy uptime-kuma:3001
}
